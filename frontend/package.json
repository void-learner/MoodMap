{
  "name": "moodmap-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Frontend for MoodMap emotion analysis chatbot",
  "main": "src/main.ts",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:start": "concurrently -k \"cross-env BROWSER=none vite\" \"wait-on http://localhost:5173 && electron ./electron/main.js\"",
    "electron:package:mac": "vite build && electron-builder -m -c.extraMetadata.main=dist/main.js",
    "electron:package:win": "vite build && electron-builder -w -c.extraMetadata.main=dist/main.js",
    "electron:package:linux": "vite build && electron-builder -l -c.extraMetadata.main=dist/main.js",
    "electron:build": "vite build && electron-builder"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "lucide-react": "^0.446.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^38.1.2",
    "electron-builder": "^24.13.3",
    "eslint": "^9.11.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.3",
    "vite": "^7.1.7",
    "wait-on": "^7.2.0"
  }
}
